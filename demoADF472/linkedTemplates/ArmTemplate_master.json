{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "demoADF472"
		},
		"ls_adlsgen2_arunkumar123_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_adlsgen2_arunkumar123'"
		},
		"ls_sql_dw_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'ls_sql_dw'"
		},
		"ls_adlsgen2_arunkumar123_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://arunkumar123.dfs.core.windows.net/"
		},
		"ls_sql_dw_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "demoserver472.database.windows.net"
		},
		"ls_sql_dw_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "demodw"
		},
		"ls_sql_dw_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "arun"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "demoADF472_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_adlsgen2_arunkumar123_accountKey": {
						"value": "[parameters('ls_adlsgen2_arunkumar123_accountKey')]"
					},
					"ls_sql_dw_password": {
						"value": "[parameters('ls_sql_dw_password')]"
					},
					"ls_adlsgen2_arunkumar123_properties_typeProperties_url": {
						"value": "[parameters('ls_adlsgen2_arunkumar123_properties_typeProperties_url')]"
					},
					"ls_sql_dw_properties_typeProperties_server": {
						"value": "[parameters('ls_sql_dw_properties_typeProperties_server')]"
					},
					"ls_sql_dw_properties_typeProperties_database": {
						"value": "[parameters('ls_sql_dw_properties_typeProperties_database')]"
					},
					"ls_sql_dw_properties_typeProperties_userName": {
						"value": "[parameters('ls_sql_dw_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}